   journal = {Industrial Marketing Management},
   keywords = {Innovation networks,Innovation promoters,Open innovation},
   month = {4},
   pages = {525-534},
   publisher = {Elsevier Inc.},
   title = {Innovation promoters - A multiple case study},
   volume = {43},
